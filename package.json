  "dependencies": {
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@headlessui/react": "^1.7.14",
    "@tanstack/react-query": "4.29.5",
    "antd": "^5.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.0"
  },
