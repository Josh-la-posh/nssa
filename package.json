{
  "name": "oponeko-super-admin-frontend-web-app",
  "version": "1.0.0",
  "description": "**Edit a file, create a new file, and clone from Bitbucket in under 2 minutes**",
  "main": "main.tsx",
  "type": "module",
  "engines": {
    "node": ">=16.0.0",
    "npm": "please-use-yarn",
    "yarn": ">1.19.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint --fix --ext .js,.ts,.tsx ./src --ignore-path .gitignore --max-warnings 0",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bigbimbs@bitbucket.org/bimbs-tech/oponeko-frontend-super-admin.git"
  },
